{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "type-check": {
      "outputs": ["*.tsbuildinfo"]
    },
    "build": {
      "dependsOn": ["type-check"],
      "outputs": ["dist/**", "public/**"],
      "env": []
    },
    "lint": {
      "outputs": [],
      "inputs": [".eslintrc.*", "src/**"]
    }
  }
}
