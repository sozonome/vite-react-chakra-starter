{
  "name": "vite-react-chakra-starter",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "biome:check": "pnpm biome check",
    "biome:fix": "pnpm biome check --write",
    "turbo": "pnpm dlx turbo@2.0.9",
    "type:check": "tsc",
    "test": "vitest run",
    "test:ui": "vitest --ui --coverage",
    "test:coverage": "vitest --coverage run",
    "check:turbo": "pnpm turbo biome:check type:check test",
    "knip": "knip",
    "up-interactive": "pnpm up -i",
    "up-latest": "pnpm up-interactive -L",
    "release": "cross-env HUSKY=0 commit-and-tag-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.8.2",
    "@emotion/react": "^11.13.3",
    "@emotion/styled": "^11.13.0",
    "@fontsource/plus-jakarta-sans": "^5.1.0",
    "framer-motion": "^11.5.4",
    "million": "3.1.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.1",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/types": "^19.5.0",
    "@types/node": "^22.5.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/react-helmet": "^6.1.11",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.1.1",
    "@vitest/ui": "^2.1.1",
    "commit-and-tag-version": "^12.4.4",
    "cross-env": "^7.0.3",
    "husky": "^9.1.6",
    "knip": "^5.30.2",
    "lint-staged": "^15.2.10",
    "rollup-plugin-visualizer": "^5.12.0",
    "typescript": "5.6.2",
    "vite": "^5.4.5",
    "vite-plugin-checker": "^0.8.0",
    "vite-plugin-pwa": "^0.20.5",
    "vite-tsconfig-paths": "^5.0.1",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": ">=20.14.x",
    "pnpm": ">=9"
  },
  "packageManager": "pnpm@9.1.1"
